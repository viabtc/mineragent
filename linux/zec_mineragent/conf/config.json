{
    "process": {
        "file_limit": 1000000,
        "core_limit": 1000000000
    },
    "log": {
        "path": "log/mineragent",
        "flag": "fatal,error,warn,info,debug",
        "num": 20
    },
    "svr": {
        "bind": [
            "tcp@0.0.0.0:5557",
            "tcp@0.0.0.0:447",
            "tcp@0.0.0.0:27"
        ],
        "max_pkg_size": 10240
    },
    "cli": "tcp@127.0.0.1:10001",
    "stratum_server": [
        {
            "host": "mining.viabtc.io",
            "port": 3002,
            "is_ssl": true
        },
        {
            "host": "mining.viabtc.io",
            "port": 302,
            "is_ssl": false
        }
    ],
    "worker_num": 8,
    "diff_min": 0.04,
    "diff_max": 16,
    "diff_default": 0.05,
    "connect_timeout": 600,
    "broadcast_timeout": 55,
    "client_max_idle_time": 86400 
}
